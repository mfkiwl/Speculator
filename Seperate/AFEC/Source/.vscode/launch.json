{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Debug Crawler",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../Dist/Debug/Crawler",
            "args": [
                "-o",
                "${workspaceFolder}/Crawler/XUnitTests/Resources/Kicks-vs-Snare-Train/afec.db",
                "${workspaceFolder}/Crawler/XUnitTests/Resources/Kicks-vs-Snare-Train/"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../Dist/Debug",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Debug ModelCreator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../Dist/Debug/ModelCreator",
            "args": [
                "-o",
                "${workspaceFolder}/Crawler/XUnitTests/Resources/Kicks-vs-Snare-Train/Kicks-vs-Snares.model",
                "${workspaceFolder}/Crawler/XUnitTests/Resources/Kicks-vs-Snare-Train/afec-ll.db"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../Dist/Debug",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}